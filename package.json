    "https": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "homebridge": "^1.3.1",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.2"
  }
